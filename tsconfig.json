{
    "compilerOptions": {
        "sourceMap": true,
        "outDir": "./dist",
        "declaration": true,
        "target": "es5",
        "strictFunctionTypes": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "lib": ["esnext", "dom"],
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "downlevelIteration": true
    },
    "include": ["lib/**/*"],
    "exclude": []
}